#VRML V2.0 utf8

Shape{
  appearance Appearance{
    material Material{
      diffuseColor 1.0 0.0 1.0
    }
  }
  geometry Cylinder {
  	radius 2.0   # raio
  	height 2.5   # altura
  	side TRUE    # tem a lateral ?
  	top TRUE     # tem a parte de cima ?
  	bottom TRUE  # tem a parte de baixo ?
  }
}
